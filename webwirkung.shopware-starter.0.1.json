{
    "manifests": {
        "webwirkung/shopware-starter": {
            "manifest": {
                "copy-from-recipe": {
                    "root/": ""
                },
                "gitignore": [
                    "/public/asset-manifest.json",
                    "!custom/apps/",
                    ".idea/",
                    "!/files/theme-config",
                    "*.sql",
                    "*.gz",
                    "*.bak",
                    "**/.DS_Store",
                    "!custom/plugins",
                    "!custom/plugins/.gitkeep",
                    "!custom/plugins/.gitignore,",
                    "!/var/plugins.json",
                    "!/var/theme-files.json",
                    "!/var/theme-variables.scss",
                    "*/node_modules"
                ],
                "post-install-output": [
                    "  * <fg=green>Let's rock!</>"
                ]
            },
            "files": {
                "root/docker-compose.override.yml": {
                    "contents": [
                        "version: '3'",
                        "services:",
                        "   mailer:",
                        "       image: schickling/mailcatcher",
                        "       ports: [\"1025\", \"1080\"]",
                        "",
                        "   database:",
                        "       image: mariadb:latest",
                        "       volumes:",
                        "           - \"db-data:/var/lib/mysql\"",
                        "       environment:",
                        "           - MARIADB_ROOT_PASSWORD=root",
                        "           - MARIADB_DATABASE=shopware,",
                        "           - MARIADB_USER=root",
                        "           - MARIADB_PASSWORD=root",
                        "       ports:",
                        "           - \"3306:3306\"",
                        "",
                        "   opensearch:",
                        "       ports:",
                        "           - \"9200\"",
                        "",
                        "       profiles:",
                        "           - donotstart",
                        "",
                        "volumes:",
                        "   db-data:"
                    ],
                    "executable": false
                },
                "root/.env.local": [
                    "SHOPWARE_SKIP_WEBINSTALLER=\"1\"",
                    "COMPOSER_PLUGIN_LOADER=\"1\""
                ]
            },
            "ref": "0eedc8011c353b79f29f845022ec0f84c62b5008"
        }
    }
}
