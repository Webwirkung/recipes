                        "           - \"3306:3306\"",
                        "",
                        "   opensearch:",
                        "       ports:",
                        "           - \"9200\"",
                        "",
                        "       profiles:",
                        "           - donotstart,",
                        "",
                        "   volumes:",
                        "       db-data:"
