{
    "manifests": {
        "webwirkung/shopware-starter": {
            "manifest": {
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "gitignore": [
                    "/public/asset-manifest.json",
                    "!custom/apps/",
                    ".idea/",
                    "!/files/theme-config",
                    "*.sql",
                    "*.gz",
                    "*.bak",
                    "**/.DS_Store"

                ],
                "docker-compose": {
                    "docker-compose.override.yml": {
                        "services": [
                            "database:",
                                "image: mariadb:latest",
                            "volumes:",
                                "- \"db_data:/var/lib/mysql\"",
                            "restart: always",
                            "environment:",
                                "- MARIADB_ROOT_PASSWORD=root",
                                "- MARIADB_DATABASE=shopware",
                                "- MARIADB_USER=root",
                                "- MARIADB_PASSWORD=root",
                            "ports:",
                                "- \"3306:3306\""
                        ]
                    }
                },
                "post-install-output": [
                    "  * <fg=blue>Setup</>",
                    "    Test "
                ]
            },
            "ref": "9c1b29f4e3f33f4ca2da2529771c849d2bb326d8"
        }
    }
}
